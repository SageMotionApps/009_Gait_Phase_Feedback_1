{
  "user_guide_file": "Gait Phase Feedback 1 App Guide.pdf",
  "app_name": "Gait Phase Feedback 1",
  "app_description": "Compute gait phase and steps of both feet and provide feedback during the gait phase. Feedback starts when gait transitions to early stance or to early, middle and late stance, and lasts for the Feedback Pulse Length. <br><br>Nodes Required: 4 <br> -Sensing (2): foot_left (top, switch pointing forward), foot_right (top, switch pointing forward) <br> -Feedback (2): feedback shank_left, feedback shank_right",
  "app_id": "009",
  "app_version": "1_3",
  "version_id": "00913",
  "parameters_setting": [
    ["Feedback Setting",
      {"source": "left_feedback_enabled", "title": "Left Foot Feedback On?", "type": "bool",
       "description": "Enable/Disable feedback"},
      {"source": "right_feedback_enabled", "title": "Right Foot Feedback On?", "type": "bool",
       "description": "Enable/Disable feedback"},
      {"source": "whenFeedback", "title": "When to Give Feedback?", "type": "combo", "validation": ["Early stance", "Early, Middle and Late stance"], 
       "description": "Specify when to give feedback"},
      {"source": "pulse_length", "title": "Feedback Pulse Length (sec)", "type": "float",
       "description": "The duration of the feedback in seconds"}],       
    ["Save Options",
      {"source": "save_mode", "title": "Save Mode", "type": "combo", "validation": ["csv", "h5", "xlsx"],
       "description": "Save file in specified format"}]
  ],
  "user_fields": [{"name": "time", "data_type": "float", "size":1},
                  {"name": "Step_Count_Left", "data_type": "int", "size":1},
                  {"name": "Gait_Phase_Left", "data_type": "int", "size":1},
                  {"name": "Step_Count_Right", "data_type": "int", "size":1},
                  {"name": "Gait_Phase_Right", "data_type": "int", "size":1},
                  {"name": "Feedback_Left", "data_type": "int", "size":1},
                  {"name": "Feedback_Right", "data_type": "int", "size":1}],

  "sensors": ["foot_left", "foot_right"],
  "feedback": ["shank_left", "shank_right"],
  "datarate": 100
}